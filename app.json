{
  "expo": {
    "name": "Optica",
    "slug": "Optica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconOptica.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconOptica.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juanesbrice.Optica",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8e7a08a6-669f-4298-9c87-8ffbef990dda"
      }
    }
  }
}
